    <br><br>


    <div data-aos="fade-up" data-aos-duration="1000" style="text-align:center">
      <span style="font-size:32px">目前開課</span><br>
      <div class="main" style="margin-top: 10vh;">
        <div class="slider slider-nav center">



                @{
                    bool flag = false;
                    foreach (var item in @ViewBag.Class)
                    {
                        foreach (var item2 in @ViewBag.ClassTime)
                        {
                            foreach (var item3 in @ViewBag.LC)
                            {
                                if (@item.LcId == @item3.LcId && @item.LcId < 4)
                                {
                                    if (@item.CId == @item2.CId)
                                    { @if (@DateTime.Now <
                     @item2.CtDate)
                                        {
                                                <div style="position: relative;">
                                            <span class="class-text">
                                                        @item.CName
                                            </span>
                                                    @foreach (var item4 in @ViewBag.ClassI)
                                                {
                                                        @if (@item4.CId == @item.CId)
                                                    {
                                                        if (@item4.InPhoto != null && flag == false)
                                                        {
                                                                    <img class="class-img" style="width: 200px; height:200px" src=@item4.InPhoto alt="">
                                                            flag = true;
                                                        }

                                                    }
                                                }
                                                    @if (flag == false)
                                                {

                                                <img class="class-img" style="width: 100%;" src="https://picsum.photos/200/200/?random=1" alt="">
                                                }
                                                                                @{flag = false;}
                                        </div>
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
        <div style="position: relative;">
          <span class="class-text">
          </span>
          <img class="class-img" style="width: 600px; height:200px" src="../image/transparent.png" alt="">
        </div>
      </div>
      <div class="slider slider-for" style="width: 100%;">
                @{
                    foreach (var item in @ViewBag.Class)
                    {
                        foreach (var item2 in @ViewBag.ClassTime)
                        {
                            foreach (var item3 in @ViewBag.LC)
                            {

                                if (@item.LcId == @item3.LcId && @item.LcId < 4)
                                {
                                    if (@item.CId == @item2.CId)
                                    { @if (@DateTime.Now <
          @item2.CtDate)
                                        {
                                            string date = @item2.CtDate.ToString();
                                                <div class="class-text2">
                                                    @foreach (var item4 in @ViewBag.ClassI)
                                                {
                                                        @if (@item4.CId == @item.CId)
                                                    {
                                                            @item4.InContent

                                                  <br>
                                                    }
                                                }
                                          價錢：@item.Cprice<br>
                                          堂數：@item.CTotalLession<br>
                                          人數上限：@item.CExpect<br>
                                          開課日：@date.Substring(0,date.Length - 12)
                                      </div>
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
      }
      <div class="class-text2"></div>
